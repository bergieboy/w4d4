
<h2>Log In</h2>

<form class="" action="<%= session_url %>" method="post">
  <%= auth_token %>
  
  <label>Email:
    <input type="text" name="user[email]">
  </label>

  <br>
  <label>Password:
    <input type="password" name="user[password]">
  </label>

  <br>
  <br>
  <input type="submit" value="Log In">

</form>
